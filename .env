# LiveKit Cloud credentials (get from https://cloud.livekit.io -> Settings -> Keys)
LIVEKIT_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=
# LiveKit Cloud WebSocket URL for the browser client (same wss:// URL as LIVEKIT_URL)
NEXT_PUBLIC_LIVEKIT_URL=

LLAMA_BASE_URL=http://llama_cpp:11434/v1
LLAMA_MODEL=qwen3-4b
LLAMA_MODEL_ALIAS=qwen3-4b
LLAMA_HF_REPO=unsloth/Qwen3-4B-Instruct-2507-GGUF
